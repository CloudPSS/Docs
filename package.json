{
  "name": "docs",
  "version": "5.3.28",
  "homepage": "https://docs.cloudpss.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/CloudPSS/Docs"
  },
  "type": "module",
  "scripts": {
    "start": "pnpm prebuild && ng serve",
    "build:main": "pnpm build --configuration production",
    "build:github": "pnpm build --configuration production,for-github",
    "build:standalone": "pnpm build --configuration production,for-standalone",
    "prebuild": "pnpm clean && pnpm build-manifest",
    "build": "ng build",
    "postbuild": "node ./tools/post-build.js",
    "serve": "http-server -p4000 ./dist/",
    "analyze": "webpack-bundle-analyzer --mode=static --default-sizes=gzip --report=dist/report.html dist/stats.json",
    "clean": "rimraf dist docs/manifest.json",
    "build-manifest": "node -e \"import('./tools/build-manifest.js').then(({default: d}) => d())\"",
    "convert-webp": "node -e \"import('./tools/convert-webp.js').then(({default: d}) => d('docs'))\"",
    "prepare": "husky install || echo Failed to initialize husky",
    "format": "prettier --cache --cache-strategy content --write .",
    "lint": "ng lint --cache --cache-location node_modules/.cache/eslint/ --cache-strategy content"
  },
  "private": true,
  "prettier": "@cloudpss/prettier-config",
  "commitlint": {
    "extends": "@cloudpss"
  },
  "dependencies": {
    "@angular/animations": "^17.0.4",
    "@angular/cdk": "^17.0.1",
    "@angular/common": "^17.0.4",
    "@angular/compiler": "^17.0.4",
    "@angular/core": "^17.0.4",
    "@angular/forms": "^17.0.4",
    "@angular/material": "^17.0.1",
    "@angular/platform-browser": "^17.0.4",
    "@angular/platform-browser-dynamic": "^17.0.4",
    "@angular/router": "^17.0.4",
    "@angular/service-worker": "^17.0.4",
    "@cloudpss/web-components": "^0.8.0",
    "@ngx-translate/core": "^15.0.0",
    "js-yaml": "^4.1.0",
    "material-icons": "^1.13.12",
    "photoswipe": "^5.4.2",
    "rxjs": "~7.8.1",
    "sanitize.css": "^13.0.0",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1700.3",
    "@angular-devkit/build-angular": "^17.0.3",
    "@angular-devkit/schematics": "^17.0.3",
    "@angular-eslint/builder": "^17.1.0",
    "@angular-eslint/eslint-plugin": "^17.1.0",
    "@angular-eslint/eslint-plugin-template": "^17.1.0",
    "@angular-eslint/schematics": "^17.1.0",
    "@angular-eslint/template-parser": "^17.1.0",
    "@angular/cli": "^17.0.3",
    "@angular/compiler-cli": "^17.0.4",
    "@cloudpss/commitlint-config": "^2.0.0",
    "@cloudpss/eslint-config": "^2.0.0",
    "@cloudpss/lint-staged-config": "^2.0.0",
    "@cloudpss/prettier-config": "^2.0.0",
    "@cloudpss/tsconfig": "^2.0.0",
    "@types/download": "^8.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash-es": "^4.17.12",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^20.9.4",
    "@types/punycode": "^2.1.3",
    "@types/webpack": "^5.28.5",
    "chalk-template": "^1.1.0",
    "download": "^8.0.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "husky": "^8.0.3",
    "image-size": "^1.0.2",
    "katex": "^0.16.9",
    "markdown-it": "^13.0.2",
    "markdown-it-front-matter": "^0.2.3",
    "minimatch": "^9.0.3",
    "pdf-lib": "^1.17.1",
    "pretty-bytes": "^6.1.1",
    "puppeteer": "^21.5.2",
    "rimraf": "^5.0.5",
    "type-fest": "^4.8.2",
    "typescript": "~5.2.2"
  }
}
