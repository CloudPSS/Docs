{
  "name": "docs",
  "version": "5.3.22",
  "homepage": "https://docs.cloudpss.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/CloudPSS/Docs"
  },
  "scripts": {
    "start": "yarn prebuild && ng serve",
    "build:main": "yarn build --configuration production",
    "build:github": "yarn build --configuration production,for-github",
    "build:standalone": "yarn build --configuration production,for-standalone",
    "prebuild": "yarn clean && yarn build-manifest",
    "build": "ng build",
    "postbuild": "node ./tools/post-build.js",
    "serve": "http-server -p4000 ./dist/",
    "lint": "ng lint",
    "format": "prettier --write .",
    "analyze": "webpack-bundle-analyzer --mode=static --default-sizes=gzip --report=dist/report.html dist/stats.json",
    "clean": "rimraf dist docs/manifest.json",
    "build-manifest": "node tools/build-manifest.js",
    "prepare": "husky install"
  },
  "private": true,
  "prettier": "@cloudpss/prettier-config",
  "commitlint": {
    "extends": "@cloudpss"
  },
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/material": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@angular/service-worker": "^14.1.0",
    "@cloudpss/web-components": "^0.6.6",
    "@ngx-translate/core": "^14.0.0",
    "js-yaml": "^4.1.0",
    "material-icons": "^1.13.9",
    "rxjs": "~7.8.1",
    "sanitize.css": "^13.0.0",
    "tslib": "^2.6.1",
    "zone.js": "~0.11.7"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^14.0.0",
    "@angular-devkit/architect": "^0.1401.0",
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular-devkit/schematics": "^14.1.0",
    "@angular-eslint/builder": "^14.0.2",
    "@angular-eslint/eslint-plugin": "^14.0.2",
    "@angular-eslint/eslint-plugin-template": "^14.0.2",
    "@angular-eslint/schematics": "^14.0.2",
    "@angular-eslint/template-parser": "^14.0.2",
    "@angular/cli": "^14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@cloudpss/commitlint-config": "^1.5.5",
    "@cloudpss/eslint-config": "^1.5.5",
    "@cloudpss/lint-staged-config": "^1.5.5",
    "@cloudpss/prettier-config": "^1.5.5",
    "@cloudpss/tsconfig": "^1.5.5",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^18.6.2",
    "@types/punycode": "^2.1.0",
    "@types/webpack": "^5.28.1",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "husky": "^8.0.3",
    "type-fest": "^2.17.0",
    "typescript": "~4.7.4",
    "webpack-bundle-analyzer": "^4.9.0",
    "yaml-loader": "^0.8.0"
  }
}
