    "@angular/compiler-cli": "^14.0.2",
    "@cloudpss/commitlint-config": "^1.1.7",
    "@cloudpss/eslint-config": "^1.1.7",
    "@cloudpss/lint-staged-config": "^1.1.7",
    "@cloudpss/prettier-config": "^1.1.7",
    "@cloudpss/tsconfig": "^1.1.7",
    "@types/chart.js": "^2.9.37",
    "@types/file-saver": "^2.0.5",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
