{
  "name": "CloudPss.doc",
  "private": true,
  "hexo": {
    "version": "3.7.1"
  },
  "scripts": {
    "build": "hexo g",
    "clean": "rm nohup.out && hexo cl",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "nohup hexo s &",
    "stop": "kill `ps -ef | grep hexo | grep -v grep | awk '{print $2}'`"
  },
  "dependencies": {
    "@iktakahiro/markdown-it-katex": "github:OpportunityLiu/markdown-it-katex#patch-1",
    "hexo": "^3.2.0",
    "hexo-generator-search": "^2.2.5",
    "hexo-renderer-ejs": "^0.3.0",
    "hexo-renderer-markdown-it-plus": "^1.0.3",
    "hexo-renderer-sass": "^0.3.2",
    "hexo-renderer-stylus": "^0.3.3",
    "hexo-renderer-ts": "^1.1.2",
    "hexo-server": "^0.2.0",
    "hexo-tag-owl": "^1.2.2",
    "katex": "^0.10.0-beta",
    "typescript": "^3.0.1",
    "vue": "^2.5.17"
  }
}
