{
  "name": "docs",
  "version": "5.3.26-alpha.2",
  "homepage": "https://docs.cloudpss.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/CloudPSS/Docs"
  },
  "type": "module",
  "scripts": {
    "start": "yarn prebuild && ng serve",
    "build:main": "yarn build --configuration production",
    "build:github": "yarn build --configuration production,for-github",
    "build:standalone": "yarn build --configuration production,for-standalone",
    "prebuild": "yarn clean && yarn build-manifest",
    "build": "ng build",
    "postbuild": "node ./tools/post-build.js",
    "serve": "http-server -p4000 ./dist/",
    "lint": "ng lint",
    "format": "prettier --write .",
    "analyze": "webpack-bundle-analyzer --mode=static --default-sizes=gzip --report=dist/report.html dist/stats.json",
    "clean": "rimraf dist docs/manifest.json",
    "build-manifest": "node -e \"import('./tools/build-manifest.js').then(({default: d}) => d())\"",
    "convert-webp": "node -e \"import('./tools/convert-webp.js').then(({default: d}) => d('docs'))\"",
    "prepare": "husky install"
  },
  "private": true,
  "prettier": "@cloudpss/prettier-config",
  "commitlint": {
    "extends": "@cloudpss"
  },
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@angular/service-worker": "^16.1.8",
    "@cloudpss/web-components": "^0.6.10",
    "@ngx-translate/core": "^15.0.0",
    "@pdf-lib/fontkit": "^1.1.1",
    "js-yaml": "^4.1.0",
    "material-icons": "^1.13.9",
    "minimatch": "^9.0.3",
    "pdf-lib": "^1.17.1",
    "pdfjs": "^2.5.2",
    "puppeteer": "^21.0.2",
    "rxjs": "~7.8.1",
    "sanitize.css": "^13.0.0",
    "tslib": "^2.6.1",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^16.0.0",
    "@angular-devkit/architect": "^0.1602.0",
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-devkit/schematics": "^16.2.0",
    "@angular-eslint/builder": "^16.1.0",
    "@angular-eslint/eslint-plugin": "^16.1.0",
    "@angular-eslint/eslint-plugin-template": "^16.1.0",
    "@angular-eslint/schematics": "^16.1.0",
    "@angular-eslint/template-parser": "^16.1.0",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@cloudpss/commitlint-config": "^1.5.6",
    "@cloudpss/eslint-config": "^1.5.6",
    "@cloudpss/lint-staged-config": "^1.5.6",
    "@cloudpss/prettier-config": "^1.5.6",
    "@cloudpss/tsconfig": "^1.5.6",
    "@types/download": "^8.0.2",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^20.4.9",
    "@types/punycode": "^2.1.0",
    "@types/webpack": "^5.28.1",
    "download": "^8.0.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.3",
    "husky": "^8.0.3",
    "image-size": "^1.0.2",
    "type-fest": "^4.2.0",
    "typescript": "~5.1.6",
    "webpack-bundle-analyzer": "^4.9.0",
    "yaml-loader": "^0.8.0"
  }
}
