{
  "name": "docs",
  "version": "5.3.13",
  "scripts": {
    "start:ng": "ng serve --disable-host-check",
    "start:docs": "yarn build-manifest && http-server -p4201 --cors -c-1 .",
    "start": "concurrently yarn:start:* --raw",
    "build:main": "yarn build --configuration production",
    "build:github": "yarn build --configuration production,for-github",
    "build:standalone": "yarn build --configuration production,for-standalone",
    "prebuild": "yarn clean",
    "build": "ng build",
    "postbuild": "node ./tools/post-build.js",
    "serve": "http-server -p4000 ./dist/",
    "lint": "ng lint",
    "format": "prettier --write .",
    "analyze": "webpack-bundle-analyzer --mode=static --default-sizes=gzip --report=dist/report.html dist/stats.json",
    "clean": "rimraf dist docs/manifest.json",
    "build-manifest": "node tools/build-manifest.js",
    "prepare": "husky install"
  },
  "private": true,
  "prettier": "@cloudpss/prettier-config",
  "commitlint": {
    "extends": "@cloudpss"
  },
  "dependencies": {
    "@angular/animations": "^14.0.2",
