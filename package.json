{
  "name": "cloudpss.doc",
  "private": true,
  "hexo": {
    "version": "3.7.1"
  },
  "scripts": {
    "build": "gulp generate",
    "clean": "rm nohup.out -f && hexo cl",
    "prestart": "npm run clean",
    "start": "nohup hexo s &",
    "stop": "kill `ps -ef | grep hexo | grep -v grep | awk '{print $2}'` > /dev/null 2> /dev/null || :",
    "deploy": "gulp",
    "postinstall": "cd ./node_modules/markdown-it-mermaid && yarn && yarn build && cd ../.."
  },
  "dependencies": {
    "@iktakahiro/markdown-it-katex": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-uglify": "^3.0.2",
    "hexo": "github:OpportunityLiu/hexo#patch-1",
    "hexo-autoprefixer": "^2.0.0",
    "hexo-deployer-rsync": "^1.0.0",
    "hexo-renderer-ejs": "^1.0.0",
    "hexo-renderer-markdown-it-plus": "^1.0.4",
    "hexo-renderer-stylus": "^1.1.0",
    "hexo-renderer-ts": "^1.2.5",
    "hexo-server": "^1.0.0",
    "hexo-tag-owl": "github:m80126colin/hexo-tag-owl",
    "katex": "^0.11.1",
    "markdown-it-center-text": "^1.0.4",
    "markdown-it-chart": "^0.2.0",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-kbd": "^2.0.0",
    "markdown-it-mermaid": "github:OpportunityLiu/markdown-it-mermaid#patch-1",
    "markdown-it-multimd-table": "^4.0.0",
    "pinyinjs": "^2.2.1",
    "puppeteer": "^2.0.0",
    "sanitize-filename": "^1.6.3",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3",
    "vue": "^2.6.11",
    "workbox-build": "^4.3.1"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.6",
    "@types/gulp-htmlmin": "^1.3.32",
    "@types/gulp-sourcemaps": "0.0.32",
    "@types/gulp-uglify": "^3.0.6",
    "@types/hexo": "^3.8.2",
    "@types/photoswipe": "^4.0.29",
    "@types/puppeteer": "^2.0.0"
  }
}
