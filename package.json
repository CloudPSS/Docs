    "markdown-it-source-map": "^0.1.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "material-icons": "^1.11.2",
    "mermaid": "^9.1.2",
    "monaco-markdown": "^0.0.12",
