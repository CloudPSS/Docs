{
  "name": "CloudPss.doc",
  "private": true,
  "hexo": {
    "version": "3.7.1"
  },
  "scripts": {
    "build": "hexo g",
    "clean": "rm nohup.out -f && hexo cl",
    "rebuild": "npm run clean && npm run build",
    "prestart": "hexo g --force",
    "start": "nohup hexo s &",
    "stop": "kill `ps -ef | grep hexo | grep -v grep | awk '{print $2}'` > /dev/null 2> /dev/null || :"
  },
  "dependencies": {
    "@iktakahiro/markdown-it-katex": "^3.0.4",
    "hexo": "github:OpportunityLiu/hexo#patch-1",
    "hexo-autoprefixer": "^1.0.0",
    "hexo-renderer-ejs": "^0.3.0",
    "hexo-renderer-markdown-it-plus": "^1.0.4",
    "hexo-renderer-sass": "^0.3.2",
    "hexo-renderer-stylus": "^0.3.3",
    "hexo-renderer-ts": "^1.2.5",
    "hexo-server": "^0.2.0",
    "hexo-tag-owl": "github:m80126colin/hexo-tag-owl",
    "katex": "^0.10.0-rc.1",
    "markdown-it-center-text": "^1.0.4",
    "markdown-it-chart": "^0.2.0",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-kbd": "^1.1.1",
    "markdown-it-mermaid": "github:OpportunityLiu/markdown-it-mermaid#patch-1",
    "markdown-it-multimd-table": "^3.1.3",
    "pinyinjs": "^2.2.1",
    "vue": "^2.5.17"
  }
}
