{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/manifest.webmanifest", "/*.css", "/*.js", "!/*-es5*.js"]
      }
    },
    {
      "name": "app-lazy",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/*-es5*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**", "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "docs",
      "urls": ["**/*.md"],
      "cacheConfig": {
        "maxAge": "100d",
        "maxSize": 1000,
        "timeout": "1s",
        "strategy": "freshness"
      }
    },
    {
      "name": "docs-assets",
      "urls": ["**/*.*"],
      "cacheConfig": {
        "maxAge": "365d",
        "maxSize": 1000,
        "timeout": "1s",
        "strategy": "freshness"
      }
    }
  ]
}
