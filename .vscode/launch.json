{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Gulp generatePdf",
            "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
            "args": [
                "generatePdf"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "hexo",
            "program": "${workspaceFolder}/node_modules/hexo/node_modules/hexo-cli/bin/hexo",
            "stopOnEntry": true,
            "args": [
                "generate"
            ]
        }
    ]
}